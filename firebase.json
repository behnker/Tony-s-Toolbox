{
  "firestore": {
    "database": "(default)",
    "location": "eur3",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "apphosting": [
    {
      "backends": [
        "studio"
      ]
    },
    {
      "backendId": "tonys-toolbox-backend",
      "rootDir": "/",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ],
  "emulators": {
    "apphosting": {
      "port": 5004,
      "host": "0.0.0.0",
      "rootDirectory": "./",
      "startCommand": "npm run dev -- -p 9006"
    },
    "auth": {
      "port": 9101,
      "host": "0.0.0.0"
    },
    "firestore": {
      "port": 8082,
      "host": "0.0.0.0"
    },
    "storage": {
      "port": 9200,
      "host": "0.0.0.0"
    },
    "ui": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 4003
    },
    "singleProjectMode": true,
    "hub": {
      "host": "0.0.0.0",
      "port": 4403
    },
    "logging": {
      "host": "0.0.0.0",
      "port": 4503
    }
  }
}
