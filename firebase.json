{
  "firestore": {
    "database": "(default)",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "storage": {
    "rules": "storage.rules"
  },
  "apphosting": [
    {
      "backends": [
        "studio"
      ]
    },
    {
      "backendId": "tonys-toolbox-backend",
      "rootDir": "./",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "functions"
      ]
    }
  ],
  "emulators": {
    "apphosting": {
      "port": 9007,
      "host": "0.0.0.0",
      "startCommand": "npm run dev -- -p 9007"
    },
    "auth": {
      "port": 9102,
      "host": "0.0.0.0"
    },
    "firestore": {
      "port": 8084,
      "host": "0.0.0.0"
    },
    "storage": {
      "port": 9200,
      "host": "0.0.0.0"
    },
    "ui": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 4004
    },
    "singleProjectMode": true
  }
}